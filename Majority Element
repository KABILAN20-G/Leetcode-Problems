class Solution {
    public int majorityElement(int[] nums) {
        int n=nums.length;

        Arrays.sort(n);

        int ans=nums[n/2];

        for(int num:nums)
        {
            if(num==ans)
            {
                count++;
            }
        }

        if(count>n/2){
            return ans;
        }

        return -1;

    }
}
